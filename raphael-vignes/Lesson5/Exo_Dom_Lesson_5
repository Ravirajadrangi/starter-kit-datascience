    "                      \"Santé publique et médecine sociale\",\"Généralistes\",\"Médecine générale\"]\n",
    "                     )\n",
    "\n",
    "def strip_corse(val):\n",
    "    if type(val) == int:\n",
    "        return val\n",
    "    if val == 'nan':\n",
    "        return -1\n",
    "    return int(re.sub('(A|B)', '0',val))\n",
    "\n",
    "\n",
    "densite['Zone'] = densite['Zone'].apply(lambda x : x.split(' ')[0])\n",
    "densite['Zone'] = densite['Zone'].apply(strip_corse)\n",
    "corse = (densite.ix[19] + densite.ix[20])/2\n",
    "corse['Zone'] = 20\n",
    "densite.ix[19] = corse\n",
    "densite = densite.drop(20)\n",
    "densite.reset_index(inplace=True)\n",
    "\n",
    "\n",
