   "cell_type": "code",
   "execution_count": 74,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "# Colonnes du dataset à récupérer du site de la DREES\n",
    "cols = ['cpam','SERIE','prs_nat','sns_date','asu_nat','cpl_cod','ben_qlt','REM_TAU','pre_spe',\n",
    "        'pre_spe1','pre_stj1','exe_spe','exe_spe1','exe_stj1','top_slm','rem_mob','rec_mon','dep_mon','act_dnb','act_coe']\n",
    "\n",
    "# Beaucoup d'information ne nous interessent pas pour cette première question\n",
    "drop_cols = ['SERIE','prs_nat','asu_nat','cpl_cod','ben_qlt','REM_TAU','pre_spe','pre_spe1','pre_stj1','exe_spe','exe_spe1','exe_stj1',\n",
    "                  'top_slm','sns_date','rem_mob','rec_mon','act_dnb','act_coe']\n",
    "\n",
    "data_damir = []\n",
    "for i in range(1,13):\n",
    "    #Chargement du dataset\n",
    "    temp_df = pd.read_csv('csv/R2015'+str.zfill(str(i),2)+'_sanslib.CSV',skiprows = 1, engine='c', \n",
    "                          names = cols, sep= ';', low_memory=False, decimal=',')\n",
    "    # On drop les colonnes inutiles\n",
    "    for co in drop_cols:\n",
    "        temp_df.drop(co, axis=1, inplace = True)\n",
    "    # Chaque fichier CSV contient numéro de CPAM / montant du dépassement pour chaque remboursement\n",
    "    # On va donc regrouper toutes les lignes par CPAM et faire la somme sur le montant de dépassement\n",
    "    temp_df = temp_df.ix[1:]\n",
    "    temp_df['cpam'] = temp_df['cpam'].apply(lambda x : int(str(x)[:-1]))\n",
    "    temp_df['dep_mon'] = temp_df['dep_mon'].apply(lambda x : int(x.replace(',','.').split('.')[0]))\n",
    "    data_damir.append(temp_df)\n",
    "    \n",
    "data_full = pd.concat(data_damir)\n",
    "g = data_full.groupby('cpam').agg({'dep_mon':np.sum}) # DataFrame donnant le montant des dépassements de santé par CPAM"
   ]
